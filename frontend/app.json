{
  "expo": {
    "name": "baconbucks",
    "slug": "baconbucks",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/images/LOGO_DARK_PIG.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./app/assets/images/LOGO_DARK_PIG.png",
      "resizeMode": "contain",
    },
    "ios": {
      "supportsTablet": true
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
